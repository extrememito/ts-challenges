{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "object-literal-sort-keys": false,
        "no-console": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}